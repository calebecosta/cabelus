{
  "name": "cabelusapi",
  "version": "0.0.1",
  "description": "API cabelus - controle de agendamentos de cortes de cabelo em uma barbearia",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "queue": "nodemon src/queue.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "start": "node dist/server.js",
    "start:queue": "node dist/queue.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "dev:eslintCorrect": "eslint --fix src --ext .js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --forceExit",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "copy:templates": "cp -R ./src/app/views/ dist/app",
    "doc": "apidoc -i ./src/routes -o public/apidoc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:calebecosta/cabelusapi.git"
  },
  "keywords": [
    "api",
    "cabelus"
  ],
  "author": "Calebe Costa | Calebe <calebekosta@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/calebecosta/cabelusapi",
  "dependencies": {
    "@sentry/node": "^6.1.0",
    "apidoc": "^0.26.0",
    "aws-sdk": "^2.870.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bee-queue": "^1.3.1",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.1",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.17",
    "nodemailer-express-handlebars": "^4.0.0",
    "pdf-creator-node": "^2.2.2",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "youch": "^2.1.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^26.0.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "grunt-apidoc": "^0.11.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.1",
    "sucrase": "^3.17.1",
    "supertest": "^6.1.3"
  }
}
